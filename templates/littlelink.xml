<?xml version="1.0"?>
<Container version="2">
  <Name>littlelink</Name>
  <Repository>davisdre/littlelink</Repository>
  <Registry>https://hub.docker.com/r/davisdre/littlelink</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>https://github.com/bravohotel91/unraid-templates/issues/new/choose</Support>
  <Overview>A lightweight DIY alternative to services like Linktree.</Overview>
  <Category>Backup: Cloud: Productivity: Tools:</Category>
  <WebUI>http://[IP]:[PORT:80]</WebUI>
  <TemplateURL>https://github.com/bravohotel91/unraid-templates/templates/littlelink.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/bravohotel91/unraid-templates/main/templates/icons/littelink.png</Icon>
  <ExtraParams/>
  <Description></Description>
  <Config Name="WebUI" Target="80" Default="80" Mode="tcp" Description="Container Port: 80" Type="Port" Display="always" Required="true" Mask="false">80</Config>
  <Config Name="Data Storage Path" Target="/data" Default="/mnt/user/appdata/littlelink" Mode="rw" Description="Container Path: /data" Type="Path" Display="advanced" Required="true" Mask="false">/mnt/user/appdata/littlelink</Config>
</Container>